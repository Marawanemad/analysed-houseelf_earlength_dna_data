  "source": [
   "# calculate averagare \n",
   "sum_l=0 ; sum_s=0 ; l=0; s=0 ;\n",
   "for i in range(len(df)) :\n",
   "    if(df.loc[i, \"earlength\"]>10):\n",
   "        sum_l = sum_l + df.loc[i, \"earlength\"]\n",
   "        l = 1+l\n",
   "    else:\n",
   "        sum_s = sum_s + df.loc[i, \"earlength\"]\n",
   "        s = s+1\n",
   "print(\"large-eared average = \",(sum_l/l))\n",
   "print(\"small-eared average = \",(sum_s/s))"
  ]
